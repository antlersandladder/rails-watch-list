<div class="container">
  <div class="row">
    <div class= "col-md-6 mx-auto">
      <div>
      <h1>Add a movie to the list</h1>
            <%= simple_form_for ([@list, @bookmark]) do |f| %>
              <%= f.input :comment %>
              <%= f.association :movie %>
              <%= f.submit %>
            <% end %>
      </div>
    </div>
  </div>
</div>
